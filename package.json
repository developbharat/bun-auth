{
  "name": "@developbharat/bun-auth",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "bun build src/index.ts --outdir dist",
    "test": "bun test",
    "dev": "bun run --watch src/index.ts"
